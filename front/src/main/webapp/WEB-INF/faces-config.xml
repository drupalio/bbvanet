<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://svn.java.net/svn/mojarra~svn/tags/2.1.13/jsf-api/doc/web-facesconfig_2_1.xsd"
	version="2.1">

	<application>

		<resource-bundle>
			<base-name>messages</base-name>
			<var>msg</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>i18n</base-name>
			<var>i18</var>
		</resource-bundle>

		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>es</supported-locale>
		</locale-config>

		<!-- <factory> <exception-handler-factory> com.common.exceptions.CustomExceptionHandlerFactory 
			</exception-handler-factory> </factory> -->

	</application>

</faces-config>