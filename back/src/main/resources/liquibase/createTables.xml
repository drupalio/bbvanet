<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog 
     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="bbva" id="1" runAlways="true">

		<createTable tableName="PRUEBA">

			<column name="ID" type="NUMBER">
				<constraints nullable="false" primaryKey="true" />
			</column>
			<column name="NOMBRE" type="VARCHAR(40)"></column>

		</createTable>

		<insert tableName="PRUEBA">
			<column name="id">1</column>
			<column name="NOMBRE">HOOOOOLA JUAN</column>
		</insert>
		
		<!-- Table MULTVALUETYPE -->
		<createTable tableName="MULTVALUETYPE">
			<column name="ID" type="BIGINT" autoIncrement="true">
				<constraints nullable="false" primaryKey="true"
					primaryKeyName="mulValueName_pk" />
			</column>
			<column name="TYPE_NAME" type="VARCHAR(40)"></column>
		</createTable>
		<!-- Insert table MULTVALUETYPE -->
		<insert tableName="MULTVALUETYPE">
			<column name="ID">1</column>
			<column name="TYPE_NAME">QUIEROS</column>
		</insert>

		<!-- Table MULTVALUEGROUP -->
		<createTable tableName="MULTVALUEGROUP">
			<column name="ID" type="BIGINT" autoIncrement="true">
				<constraints nullable="false" primaryKey="true"
					primaryKeyName="mulValue_pk" />
			</column>
			<column name="VALUE" type="VARCHAR(40)"></column>
			<column name="TYPE_ID" type="BIGINT">
				<constraints nullable="false" />
			</column>
		</createTable>

		<addForeignKeyConstraint baseColumnNames="TYPE_ID"
			baseTableName="MULTVALUEGROUP" constraintName="fk_mulValueTypeID"
			 referencedTableName="MULTVALUETYPE" referencedColumnNames="ID"  />

		<!-- Insert table MULTVALUEGROUP -->
		<insert tableName="MULTVALUEGROUP">
			<column name="ID" autoIncrement="true" />
			<column name="VALUE">text.combo.likes.seemovs</column>
			<column name="TYPE_ID">1</column>
		</insert>
		<insert tableName="MULTVALUEGROUP">
			<column name="ID" autoIncrement="true" />
			<column name="VALUE">text.combo.likes.managed.transfer</column>
			<column name="TYPE_ID">1</column>
		</insert>
		<insert tableName="MULTVALUEGROUP">
			<column name="ID" autoIncrement="true" />
			<column name="VALUE">text.combo.likes.do.transfers</column>
			<column name="TYPE_ID">1</column>
		</insert>

	</changeSet>

</databaseChangeLog>